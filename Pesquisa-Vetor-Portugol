programa {
	funcao inicio() {
		inteiro vetor[] = {39, 45, 54, 55}
		inteiro numero
		logico achou = falso
		
		escreva ("Qual número deseja procurar? ")
		leia (numero)
		
		para (inteiro posicao = 0; posicao <=3; posicao++){
		   se (vetor[posicao] == numero)
		   {
		       escreva ("Encontrado na posição: ", posicao, "\n")
		       achou = verdadeiro
		   }
		}
		
		se (nao achou)
		{
		    escreva ("O número não está no vetor\n")
		}
	}
}
